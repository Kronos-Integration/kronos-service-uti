{
  "name": "kronos-service-uti",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/service-uti.js",
  "module": "src/service-uti.js",
  "description": "uti provider service",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/service-uti.js --section=API",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint src/service-uti.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-service": "^4.15.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "uti": "5.1.0"
  },
  "devDependencies": {
    "semantic-release": "^15.11.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.1",
    "ava": "^1.0.0-rc.2",
    "documentation": "^8.1.2",
    "travis-deploy-once": "^5.0.9",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-json": "^3.1.0",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-uti.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-uti/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-uti#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js"
    ],
    "presets": []
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
